{
    "name": "kooser/session",
    "description": "Securely manage and preserve session data.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/options-resolver": "^4.3",
        "symfony/polyfill-php72": "^1.11",
        "symfony/polyfill-php73": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Kooser\\Session\\": "./src"
        }
    },
    "suggest": {
        "symfony/cache": "If you are using a cache based session manager.",
        "kooser/directory": "If you are using a database based session manager."
    },
    "require-dev": {
        "vimeo/psalm": "^3",
        "phpunit/phpunit": "^8.2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
