{
    "name": "kooser/session",
    "description": "Securely manage and preserve session data.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Kooser\\Session\\": "./src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^3",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "symplify/coding-standard": "^5.1",
        "symplify/easy-coding-standard": "^5.1"
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check src tests bin",
        "fix-cs": "vendor/bin/ecs check src tests bin --fix",
        "phpstan": "vendor/bin/phpstan.phar analyse src bin --level max --configuration phpstan.neon"
    }
    "prefer-stable": true
}
