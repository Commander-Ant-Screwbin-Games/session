{
    "name": "kooser/session",
    "description": "Securely manage and preserve session data.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/filesystem": "^4.3",
        "symfony/polyfill-php72": "^1.11",
        "symfony/polyfill-php73": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Kooser\\Session\\": "./src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^3",
        "phpunit/phpunit": "^8.2",
        "alcaeus/mongo-php-adapter": "^1.1"
    },
    "suggest": {
        "ext-mongodb": "If you want to use the MongoDB Session Handler.",
        "ext-redis": "If you want to use the Redis Session Handler."
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
