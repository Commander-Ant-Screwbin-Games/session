{
    "name": "commander-ant-screwbin-games/session",
    "description": "Securely manage and preserve session data.",
    "homepage": "https://github.com/Commander-Ant-Screwbin-Games",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/options-resolver": "^5.0",
        "symfony/polyfill-mbstring": "^1.13",
        "symfony/polyfill-php72": "^1.13",
        "symfony/polyfill-php73": "^1.13"
    },
    "require-dev": {
        "vimeo/psalm": "^3",
        "phpunit/phpunit": "^8.4",
        "commander-ant-screwbin-games/directory": "^4.0",
        "paragonie/halite": "^4.6",
        "symfony/cache": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Session\\": "./src"
        }
    },
    "suggest": {
        "symfony/cache": "If you are using a cache based session manager.",
        "commander-ant-screwbin-games/directory": "If you are using a database based session manager.",
        "paragonie/halite": "If you are using encrypted sessions."
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
